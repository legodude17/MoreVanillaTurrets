    </projectile>
  </ThingDef>

  <ThingDef ParentName="BaseWeaponTurret">
    <defName>Gun_GunComplex</defName>
    <label>gun complex gun</label>
    <graphicData>
      <texPath>Things/Building/Security/GunComplexGun</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <description>Heavy machine gun.</description>
    <soundInteract>Interact_Rifle</soundInteract>
    <statBases>
      <Mass>40</Mass>
      <AccuracyTouch>0.6</AccuracyTouch>
      <AccuracyShort>0.45</AccuracyShort>
      <AccuracyMedium>0.35</AccuracyMedium>
      <AccuracyLong>0.30</AccuracyLong>
      <RangedWeapon_Cooldown>0.66</RangedWeapon_Cooldown>
    </statBases>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Bullet_GunComplex</defaultProjectile>
        <warmupTime>1.5</warmupTime>
        <range>32</range>
        <ticksBetweenBurstShots>5</ticksBetweenBurstShots>
        <burstShotCount>1</burstShotCount>
        <soundCast>Shot_Minigun</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>18</muzzleFlashScale>
        <consumeFuelPerShot>1</consumeFuelPerShot>
      </li>
    </verbs>
  </ThingDef>

